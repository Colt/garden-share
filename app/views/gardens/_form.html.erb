							<%= form_for @garden do |f| %>
  Address : 		<%= f.text_field :address %><br />
  City : 				<%= f.text_field :city %><br />
  Short Description: <%= f.text_field :short_description %><br />
  Description : <%= f.text_area :description %><br />
  Rules    : 		<%= f.text_area :rules %><br />
  Garden Picture: <%= f.file_field :photo %><br />
    							<%= f.submit 'submit' %>
<% end %>
